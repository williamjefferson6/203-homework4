#include <iostream>
#include <queue>
using namespace std;
int main()
{
	queue<int> q, q1;
	for (int i = 1; i <= 6; i++) {
		q.push(i);
	}
	for (int i = 0; i < q.size(); i++) {
		cout << q.front() << " ";
		q.pop();
	}
	q1 = q;
	int x = q1.size();
	for (int i = 0; i < x; i++) {
		if (i == 0) {
			cout << "{";
		}
		cout << q1.front() << ", ";
		q1.pop();
		if (i == x - 1) {
			cout << "\b\b} size " << q.size();
		}
	}
}
